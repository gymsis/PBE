create database ATENEA;
use ATENEA;
create table students(
	  uid varchar(10) PRIMARY KEY,
    nom varchar(200)
);

use ATENEA;
create table tasks(
	  date 	  varchar(10),
    subject varchar(10),
    nom		  varchar(200),
    uid		  varchar(10) 
);

use ATENEA;
create table timetables(
	  day 		  varchar(10),
    hour    	varchar(10),
    subject		varchar(10),
    room		  varchar(10)
);

use ATENEA;
create table marks(
	  uid			  varchar(10),
    subject		varchar(10),
    nom			  varchar(200),
    mark		  int
);


